{
  "name": "paste-clone",
  "version": "1.0.0",
  "description": "its like pastebin",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "NODE_ENV=production node_modules/.bin/browserify -t [reactify --es6] frontend/App.jsx -o frontend/public/app.js && node_modules/.bin/uglifyjs frontend/public/app.js -c -m -o frontend/public/app.min.js ; node_modules/.bin/stylus frontend/stylesheets/index.styl -o frontend/public",
    "watch": "node_modules/.bin/watchify -v -t [reactify --es6] frontend/App.jsx -o frontend/public/app.js",
    "watchcss": "node_modules/.bin/stylus frontend/stylesheets/index.styl -w -o frontend/public",
    "start": "NODE_ENV=dev node_modules/.bin/nodemon server/index.js"
  },
