{
  "name": "paste-clone",
  "version": "1.0.0",
  "description": "its like pastebin",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test PORT=9000 node_modules/.bin/mocha __tests__ --recursive --harmony",
    "build": "NODE_ENV=production node_modules/.bin/browserify -t [reactify --es6] frontend/App.jsx -o frontend/public/app.js && node_modules/.bin/uglifyjs frontend/public/app.js --drop-console --dead_code -c -m -o frontend/public/app.min.js ; node_modules/.bin/stylus frontend/stylesheets/index.styl -o frontend/public",
    "watch": "node_modules/.bin/watchify -v -t [reactify --es6] frontend/App.jsx -o frontend/public/app.js",
    "watchcss": "node_modules/.bin/stylus frontend/stylesheets/index.styl -w -o frontend/public",
    "start": "NODE_ENV=dev node_modules/.bin/nodemon --harmony --harmony_arrow_functions server"
  },
  "author": "Mitch Robb",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^2.4.2",
    "body-parser": "^1.10.0",
    "compression": "^1.4.0",
    "ejs": "^1.0.0",
    "express": "^4.10.6",
    "express-cors": "0.0.3",
    "fluxxor": "^1.5.1",
    "highlight.js": "^8.4.0",
    "immutable": "^3.4.1",
    "mongoose": "^3.8.21",
    "mousetrap": "^1.4.6",
    "node-jsx": "^0.12.4",
    "react": "^0.12.2",
    "serve-favicon": "^2.2.0",
    "superagent": "^0.21.0"
  },
  "devDependencies": {
    "browserify": "^8.1.3",
    "mocha": "^2.1.0",
    "nodemon": "^1.3.6",
    "reactify": "^0.17.1",
    "should": "^4.6.5",
    "sinon": "^1.12.2",
    "stylus": "^0.49.3",
    "supertest": "^0.15.0",
    "uglify-js": "^2.4.16",
    "watchify": "^2.3.0"
  },
  "engines": {
    "iojs": "1.0.x"
  }
}
